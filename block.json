{
  "apiVersion": 2,
  "name": "ace-seo/breadcrumbs",
  "title": "ACE SEO Breadcrumbs",
  "description": "Displays breadcrumb navigation generated by ACE SEO.",
  "category": "widgets",
  "icon": "menu-alt",
  "editorScript": "file:./build/index.js",
  "editorStyle": "file:./build/index.css",
  "style": "file:./build/style-index.css",
  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "textAlign": {
      "type": "string",
      "default": ""
    },
    "showHome": {
      "type": "boolean",
      "default": true
    },
    "showCurrent": {
      "type": "boolean",
      "default": true
    },
    "showLabel": {
      "type": "boolean",
      "default": false
    },
    "labelText": {
      "type": "string",
      "default": "You are here:"
    },
    "separator": {
      "type": "string",
      "default": "/"
    },
    "ariaLabel": {
      "type": "string",
      "default": "Breadcrumbs"
    }
  },
  "usesContext": [
    "postId",
    "postType",
    "queryId",
    "query",
    "termId",
    "termType",
    "taxonomy"
  ],
  "supports": {
    "html": false,
    "align": [
      "left",
      "center",
      "right",
      "wide",
      "full"
    ],
    "color": {
      "text": true,
      "link": true
    },
    "spacing": {
      "padding": true,
      "margin": [
        "top",
        "bottom"
      ]
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontWeight": true,
      "__experimentalLetterSpacing": true
    }
  }
}
